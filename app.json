{
  "expo": {
    "name": "Campus",
    "slug": "MyCampus",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#4CAF50"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#4CAF50"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tydji12.MyCampus"
    },
    "web": {
      "backgroundColor": "#4CAF50"
    },
    "plugins": [
      "expo-secure-store",
      "expo-sqlite"
    ]
  }
}
